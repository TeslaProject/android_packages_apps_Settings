<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:title="@string/system_settings_title"
    settings:keywords="@string/keywords_tesla" >

    <PreferenceScreen
        android:key="status_bar_expanded_header"
        android:fragment="com.android.settings.darkkat.StatusBarExpandedHeaderSettings"
        android:title="@string/status_bar_expanded_header_settings_title" />

   <PreferenceScreen
        android:key="status_bar"
        android:title="@string/status_bar_title"
        android:summary="@string/status_bar_summary"
        android:fragment="com.android.settings.darkkat.Statusbar" />

   <PreferenceScreen
        android:id="@+id/lock_screen"
        android:title="@string/lock_screen_title"
        android:fragment="com.android.settings.tesla.LockScreen" />

   <PreferenceScreen
        android:id="@+id/more"
        android:title="@string/more"
        android:fragment="com.android.settings.darkkat.More" />

</PreferenceScreen>
